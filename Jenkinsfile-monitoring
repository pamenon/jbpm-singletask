pipeline {
    agent { 
		label 'maven' 
	}
    parameters {
        string(name: 'Namespace', defaultValue: '', description: 'Your Paas project name:')
    }
     options {
       
        disableConcurrentBuilds()
       
        timeout(time: 15, unit: 'MINUTES')
     
    }
    stages {
        stage('build') {
            steps {
                sh 'mvn --version'
            }
        }
       
    }
}